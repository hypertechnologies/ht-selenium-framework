{
  "name" : "Selenium Hybrid Framework",
  "description": "This suite is to run test suite & test cases in multiple browsers",
  "implicitWaitTimeout": 5000,
  "explicitWaitTimeout": 5000,
  "ignoreCertificateError": true,
  "browsers": ["chrome"],
  "screenSize": "1920,1080",
  "suiteDir": "src/main/java/framework/suites",
  "suites" : [
    {
      "directory": "src/main/java/framework/suites/regression",
      "ignoreTestCases": ["OSPOS_Automation_Test_Case.xlsx"],
      "skip": false
    },
    {
      "directory": "src/main/java/framework/suites/smoke",
      "ignoreTestCases": [],
      "skip": false
    }
  ],
  "excelIndexes": {
      "tsSheetName": "TS",
      "tsIdColumnIndex": 0,
      "tsNameColumnIndex": 1,
      "tsResultColumnIndex": 2,
      "tsCommentColumnIndex": 3,
      "tsSkipColumnIndex": 4,
      "tcSheetNamePrefix": "TC-",
      "tcKeywordColumnIndex": 2,
      "tcSelectorTypeColumnIndex": 3,
      "tcSelectorValueColumnIndex": 4,
      "tcTestDataColumnIndex": 5,
      "tcResultColumnIndex": 6,
      "tcCommentColumnIndex": 7,
      "tcSkipIndex": 8
    }
}